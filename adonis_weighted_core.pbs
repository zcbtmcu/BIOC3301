#!/bin/bash
#SBATCH -t 2:00:00
#SBATCH -n 24
#SBATCH -p normal

# Load modules
module load eb
module load Miniconda2
module load R

# loading virtualenv
source activate qiime1

# setting temporary directory
export TMPDIR=~/qiime_tmp

# carrying out adonis statistical test for beta diversity
compare_categories.py \
--method adonis \
-i /home/cbmucl13/cdout_merged_core_90/weighted_unifrac_dm.txt \
-m /home/cbmucl13/map_merged_altered.txt \
-c Year \
-o /home/cbmucl13/adonis_weighted_core \
-n 99

source deactivate
